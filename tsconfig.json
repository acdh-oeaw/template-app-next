{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@acdh-oeaw/tsconfig",
	"compilerOptions": {
		"paths": {
			"@/*": ["./*"]
		},
		"plugins": [{ "name": "next" }]
	},
	"include": [
		"./next-env.d.ts",
		"./**/*.js",
		"./**/*.ts",
		"./**/*.tsx",
		"./types/**/*.d.ts",
		".next/types/**/*.ts"
	],
	"exclude": [".next", "content", "node_modules", "out", "public", "storybook-static"]
}
