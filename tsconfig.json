{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@acdh-oeaw/tsconfig",
	"compilerOptions": {
		"paths": {
			"@/*": ["./*"]
		},
		"plugins": [{ "name": "next" }]
	},
	"include": ["./**/*.d.ts", "./**/*.js", "./**/*.ts", "./**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules", "out"]
}
